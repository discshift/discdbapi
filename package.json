{
  "name": "discdbapi",
  "version": "0.1.0",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/bun": "latest"
  },
  "scripts": {
    "build:js": "bun build ./src/index.ts --outdir ./dist",
